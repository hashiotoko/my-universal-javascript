{
  "name": "@my-universal-javascript/backend",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "typecheck": "bun run tsc",
    "dev": "bun run src/index.ts",
    "typeorm": "typeorm-ts-node-esm",
    "migration:create": "bun run typeorm migration:create",
    "migration:generate": "bun run typeorm -d ./data-source.ts migration:generate -p",
    "migration:run": "bun run typeorm -d ./data-source.ts migration:run",
    "migration:revert": "bun run typeorm -d ./data-source.ts migration:revert",
    "migration:show": "bun run typeorm -d ./data-source.ts migration:show",
    "lint": "eslint './**/*.{ts,json,js}'"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^16.11.10",
    "bun-types": "^1.0.3",
    "ts-node": "^10.9.1"
  }
}
